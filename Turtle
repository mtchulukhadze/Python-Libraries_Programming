import turtle

myPen = turtle.Turtle()
myPen.shape('arrow')
myPen.color("red")

for i in range(0, 11):
    myPen.penup()
    myPen.goto(0, (10 - 1) * 20)
    myPen.pendown()
    myPen.goto(i * 20, 0)

    myPen.penup()
    myPen.goto(-(10 - 1) * 20, 0)
    myPen.pendown()
    myPen.goto(0, i * 20)


turtle.done()
